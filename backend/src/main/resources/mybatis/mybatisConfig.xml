<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias type="com.ssafy.sharehouse.dto.User" alias="User" />
		<typeAlias type="com.ssafy.sharehouse.dto.Message" alias="Message" />
		<typeAlias type="com.ssafy.sharehouse.dto.ChatRoom" alias="ChatRoom" />
		<typeAlias type="com.ssafy.sharehouse.dto.Article" alias="Article" />
		<typeAlias type="com.ssafy.sharehouse.dto.AreaCode" alias="AreaCode" />
		<typeAlias type="com.ssafy.sharehouse.dto.WishList" alias="WishList" />
		<typeAlias type="com.ssafy.sharehouse.dto.HouseRent" alias="HouseRent" />
		<typeAlias type="com.ssafy.sharehouse.dto.PageNavigation" alias="PageNavigation" />
	</typeAliases>

	<mappers>
		<mapper resource="mybatis/articleSQLMapper.xml" />
		<mapper resource="mybatis/messageSQLMapper.xml" />
		<mapper resource="mybatis/chatRoomSQLMapper.xml" />
		<mapper resource="mybatis/houseSQLMapper.xml" />
		<mapper resource="mybatis/houseSQLMapper.xml" />
		<mapper resource="mybatis/userSQLMapper.xml" />
		<mapper resource="mybatis/wishListSQLMapper.xml" />
	</mappers>
</configuration>